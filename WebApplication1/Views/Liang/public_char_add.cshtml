@model PublicCharModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>public_char_add</title>
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
</head>
<body>
    <div> 
       @using (Html.BeginForm("InsertoneWhere", "Liang", FormMethod.Post))
       {
    <table width="100%">
        <tr>
            <td>
                <font color="black">
                    您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--公共属性设置--公共属性添加
                </font>
            </td>
        </tr>
        <tr>
            <td align="right">
                <input type="submit" value="提交" class="BUTTON_STYLE1">
                <input type="button" value="返回" class="BUTTON_STYLE1"
                       onclick="history.back();">
            </td>
        </tr>
    </table>

    <table width="100%" border="1" cellpadding=0 cellspacing=1
           bordercolorlight=#848284 bordercolordark=#eeeeee
           class="TABLE_STYLE1">
        <tr>
            <td class="TD_STYLE1" width="19%">
                
                @Html.Label("属性种类")
            </td>
            <td class="TD_STYLE2" width="81%">
                @Html.TextBox("Ptype", "", new { @class = "INPUT_STYLE1", @name = "cpc.attributeKind" })
                @Html.ValidationMessage("Ptype")
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                
                @Html.Label("属性名称")
            </td>
            <td class="TD_STYLE2">
      
                @Html.TextBox("PName", "", new { @class = "INPUT_STYLE1",@name = "cpc.attributeName" })
                @Html.ValidationMessage("PName")
            </td>
        </tr>
    </table>
       }
    </div>
</body>
</html>
