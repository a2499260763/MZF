@model Model.oneorganizationModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Selectoneorganization</title>
    <link href="~/Css/table.css" rel="stylesheet" />
    <script src="~/javascript/vue-2.4.0.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>

</head>
<body>
    <div id="div123">
        
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">
                            您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置
                        </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <input type="button" value="添加" class="BUTTON_STYLE1" v-on:click="insert" />
                    </td>
                </tr>
            </table>
            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td width="20%" class="TD_STYLE1">
                        I级机构编号
                    </td>
                    <td width="20%" class="TD_STYLE1">
                        I级机构名称
                    </td>
                    <td width="25%" class="TD_STYLE1">
                        薪酬发放责任人编号
                    </td>
                    <td width="25%" class="TD_STYLE1">
                        销售责任人编号
                    </td>
                    <td width="5%" class="TD_STYLE1">
                        变更
                    </td>
                    <td width="5%" class="TD_STYLE1">
                        删除
                    </td>
                </tr>

                
                <tr v-for="(item,index) in nums">
                    <td class="TD_STYLE2">
                        {{item.oneCode}}
                    </td>
                    <td class="TD_STYLE2">
                        {{item.oneCodeName}}
                    </td>
                    <td class="TD_STYLE2">
                        {{item.oneCoderemuneration}}
                    </td>
                    <td class="TD_STYLE2">
                        {{item.oneCodemarket}}
                    </td>
                    <td class="TD_STYLE2">
                        <a v-on:click="Updateone(item.oneID)">变更</a>
                    </td>
                    <td class="TD_STYLE2">
                        <a v-on:click="Deleteone(item.oneID)">删除</a>
                    </td>
                </tr>

                


            </table>
            

       
    </div>
    <script type="text/javascript">
        var vm = new Vue({
            el: "#div123",
            data: {
                nums:""
            },
            methods: {
                load: function () {
                    var t = this;
                    $.ajax({
                        url: "/oneorganization/Selectoneorganization2",
                        type:"post",
                        success: function (msg) {
                            var dt = eval("(" + msg + ")");
                            t.nums = dt;
                        }
                    })
                },
                Updateone(oneID) {
                    location.href = '../oneorganization/SelectWhereone/' + oneID;
                },
                insert() {
                    location.href = '../oneorganization/insertone/';
                },
                Deleteone(oneID) {
                    if (confirm("确认要删除吗？")) {
                        var t = this;
                        $.ajax({
                            url: "/oneorganization/Deleteone/"+oneID,
                            type: "post",
                            success: function (msg) {
                                var dt = eval("(" + msg + ")");
                                if (dt > 0) {
                                    alert("删除成功");
                                    location.href = '../oneorganization/Selectoneorganization';
                                } else {
                                    alert("删除失败");
                                }
                            }
                        })
                    }
                    
                }
            },
            created: function () {
                this.load();
            }
        })
    </script>
</body>
</html>
