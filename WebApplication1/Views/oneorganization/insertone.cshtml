@model oneorganizationModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>insertone</title>
    <link href="~/Css/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
</head>
<body>
    <div> 
        @using (Html.BeginForm("InsertoneWhere", "oneorganization", FormMethod.Post))
        {
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置--I级机构添加
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="提交" class="BUTTON_STYLE1">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>

        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
               
                <td width="19%" class="TD_STYLE1">
                    @Html.Label("I级机构编号")
                </td>
                <td width="81%" class="TD_STYLE2">
                    @Html.TextBox("oneCode",ViewData["s"], new { @readonly = "readonly", @class = "INPUT_STYLE1" })
                 
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    @Html.Label("I级机构名称")
                </td>
                <td class="TD_STYLE2">
                    
                    @Html.TextBox("oneCodeName", "", new { @class = "INPUT_STYLE1" })
                    @Html.ValidationMessage("oneCodeName")
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    
                @Html.Label("薪酬发放责任人编号（多个编号之间请用'半角逗号'加'一个空格'隔开，如', '）")
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("oneCoderemuneration", "", new { @style = "height:80px", @class = "TEXTAREA_STYLE1"})
                   @Html.ValidationMessage("oneCoderemuneration")
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                @Html.Label("销售责任人编号（多个编号之间请用'半角逗号'加'一个空格'隔开，如', '）")
                    
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("oneCodemarket", "", new { @style = "height:80px", @class = "TEXTAREA_STYLE1" })
                    @Html.ValidationMessage("oneCodemarket")
                </td>
            </tr>
        </table>
        }
    </div>
</body>
</html>
