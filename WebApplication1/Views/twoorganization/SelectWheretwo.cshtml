@model twoorganizationModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SelectWheretwo</title>
    <link href="~/Css/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
</head>
<body>
    <div> 
        @using (Html.BeginForm("Updatetwo", "twoorganization", FormMethod.Post))
        {
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--II级机构设置--II级机构变更
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="提交" class="BUTTON_STYLE1">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>

        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <input type="hidden" name="cfsk.fskId" value=761 class="INPUT_STYLE1" />
            <tr>
                <td width="19%" class="TD_STYLE1">
                    @Html.Hidden("twoID", Model.twoID, new { @readonly = "readonly", @class = "INPUT_STYLE1" })
                    @Html.Label("I级机构编号")
                </td>
                <td width="81%" class="TD_STYLE2">
                   @Html.TextBox("oneCodeName", Model.oneCodeName, new { @readonly = "readonly", @class = "INPUT_STYLE1" })
                   @Html.ValidationMessage("oneName")
                </td>
            </tr>
            <tr>
                <td width="19%" class="TD_STYLE1">
                    @Html.Label("II级机构编号")
                </td>
                <td width="81%" class="TD_STYLE2">
                    @Html.TextBox("twoCode", Model.twoCode, new { @readonly = "readonly", @class = "INPUT_STYLE1" })
                    @Html.ValidationMessage("twoCode")
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    @Html.Label("II级机构名称")
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBox("twoCodeName", Model.twoCodeName, new { @readonly = "readonly", @class = "INPUT_STYLE1" })
                    @Html.ValidationMessage("twoCodeName")
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    @Html.Label("薪酬发放责任人编号（多个编号之间请用'半角逗号'加'一个空格'隔开，如', '）")
                </td>
                <td class="TD_STYLE2">
                    @Html.TextArea("twoCoderemuneration", Model.twoCoderemuneration, new { @rows = "4", @class = "TEXTAREA_STYLE1" })
                    @Html.ValidationMessage("twoCoderemuneration")
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    @Html.Label("销售责任人编号（多个编号之间请用'半角逗号'加'一个空格'隔开，如', '）")
                </td>
                <td class="TD_STYLE2">
                    @Html.TextArea("twoCodemarket", Model.twoCodemarket, new { @rows = "4", @class = "TEXTAREA_STYLE1" })
                    @Html.ValidationMessage("twoCodemarket")
                </td>
            </tr>
        </table>
        }
    </div>
</body>
</html>
